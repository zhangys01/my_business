<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.business.business.dao.WorkFlowOrderDao">

    <select id="findOrderById" parameterType="String" resultType="com.business.business.entity.WorkflowOrder">
		select * from workflow_order where taskSerialNumber = #{taskSerialNumber}
	</select>
	<select id="selectList" parameterType="String" resultType="com.business.business.entity.WorkflowOrder">
	select * from workflow_order where orderStatus = #{0} order by taskPriority desc,startTime asc limit 5
	</select>
</mapper>