			<productId>%PRODUCTID_L2A%</productId>
			<productId_L1A>%PRODUCTID_L1A%</productId_L1A>
			<sceneId>%SCENEID%</sceneId>
			<server_address>172.19.4.119</server_address>
			<port_number>18886</port_number>
		</params>
	</task>
